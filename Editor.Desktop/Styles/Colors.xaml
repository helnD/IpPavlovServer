<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="PrimaryColor">#002A5A</Color>

    <SolidColorBrush x:Key="PrimaryBrush" Color="{StaticResource PrimaryColor}"/>
    <SolidColorBrush x:Key="BackgroundBrush">#FAFAFA</SolidColorBrush>
    <SolidColorBrush x:Key="SelectedBrush">#DEDEDE</SolidColorBrush>
    <SolidColorBrush x:Key="SeparatorBrush">#D4D4D4</SolidColorBrush>
</ResourceDictionary>